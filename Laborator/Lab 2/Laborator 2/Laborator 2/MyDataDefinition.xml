<?xml version="1.0" encoding="utf-8" ?>
<dataDefinition>
  
  <table name="Manufacturers">
    <constraint type="primaryKey" name="pk_MaId" columns="MaId" />
    <column name="MaId" type="Int"/>
    <column name="Name" type="VarChar" />
  </table>
  
  <table name="Models">
    <constraint type="primaryKey" name="pk_MoId" columns="MoId" />
    <constraint type="foreignKey" name="" 
                column="MaId" 
                foreignTable="Manufacturers" 
                foreignColumn="MaId" />
    <column name="MoId" type="Int"/>
    <column name="Company" type="Int" />
    <column name="Name" type="VarChar" />
    <column name="ReleaseDate" type="Date" />
    <column name="NumberSeats" type="Int" />
    <column name="ChassisType" type="VarChar" />
  </table>
  
</dataDefinition>